{
  "src/components/*.js": {
    "type": "component",
    "dispatch": "yarn run test --silent=true --watchAll=false src/components/{}.test.js",
    "alternate": "src/components/{}.test.js"
  },

  "src/helpers/*.js": {
    "type": "helper",
    "alternate": "src/helpers/{}.test.js"
  },

  "src/components/*.test.js": {
    "type": "tcomponent",
    "alternate": "src/components/{}.js"
  },

  "src/helpers/*.test.js": {
    "type": "thelpers",
    "dispatch": "yarn run test --silent=true --watchAll=false src/components/{}.test.js",
    "alternate": "src/components/{}.js"
  },

  "src/index.js": {
    "type": "index"
  },

  "src/css/app.scss": {
    "type": "stylesheet"
  },

  "src/css/*.scss": {
    "type": "stylesheet"
  }
}
